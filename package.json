{
  "name": "electron-updater-example",
  "version": "0.8.6",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "publish": "electron-builder -p always",
    "start": "electron ."
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.1"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9"
  },
  "build": {
    "appId": "com.github.navneetkumar936.electron_second_updater",
    "afterSign": "./build/notarize.js",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "navneetkumar936",
        "repo": "https://github.com/navneetkumar936/electron_second_updater"
      }
    ]
  }
}